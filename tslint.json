{
  "extends": "tslint:recommended",
  "rules": {
    "no-var-requires": false,
    "quotemark": [true, "single"],
    "semicolon": false,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "no-angle-bracket-type-assertion": false,
    "no-console": false,
    "object-literal-sort-keys": false
  }
}
